#!/usr/bin/env bash

# go path
export GOPATH="$HOME/golang"
export PATH="$GOPATH/bin:/usr/local/go/bin:$PATH"

# home bin
HOMEBIN="$HOME/bin"
export PATH="$HOMEBIN:$PATH"

# puppet bin
PUPPETBIN="/opt/puppet/bin"
export PATH="$PATH:$PUPPETBIN"

# global path
export PATH="/usr/local/bin:$PATH:/sbin"
